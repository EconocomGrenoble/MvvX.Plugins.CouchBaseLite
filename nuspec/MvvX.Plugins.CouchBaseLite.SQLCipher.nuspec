<?xml version="1.0" encoding="utf-8"?>
 <package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>MvvX.Plugins.CouchBaseLite.SQLCipher</id>
        <version>{BuildNumberVersion}</version>
        <title>CouchBaseLite with SQLCipher (MvvmCross plugin)</title>
        <authors>MACK Mathieu</authors>
        <owners>MACK Mathieu</owners>
        <licenseUrl>https://github.com/mathieumack/MvvX.CouchBaseLite</licenseUrl>
        <projectUrl>https://github.com/mathieumack/MvvX.CouchBaseLite</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>This package contains the 'CouchBaseLite' plugin for MvvmCross.</description>
        <tags>mvvm mvvmcross xamarin monodroid monotouch wpf couchbase sqlcipher</tags>
        <dependencies>
            <group>
                <!-- Always include regardless of target framework -->
                <dependency id="MvvX.Plugins.CouchBaseLite" version="{BuildNumberVersion}" />
            </group>
            <group targetFramework="net45">
                <!-- Always include regardless of target framework -->
                <dependency id="MvvX.Plugins.CouchBaseLite" version="{BuildNumberVersion}" />
                <dependency id="Couchbase.Lite.Storage.SQLCipher" version="1.3.0" />
            </group>
            <group targetFramework="MonoAndroid10">
                <!-- Always include regardless of target framework -->
                <dependency id="MvvX.Plugins.CouchBaseLite" version="{BuildNumberVersion}" />
                <dependency id="Couchbase.Lite.Storage.SQLCipher" version="1.3.0" />
            </group>
            <group targetFramework="xamarinios10">
                <!-- Always include regardless of target framework -->
                <dependency id="MvvX.Plugins.CouchBaseLite" version="{BuildNumberVersion}" />
                <dependency id="Couchbase.Lite.Storage.SQLCipher" version="1.3.0" />
            </group>
        <group targetFramework="portable-win+sl50+wp8">
            <!-- Otherwise include the Portable CSL -->
        </group>
        </dependencies>  
    </metadata>
</package>

